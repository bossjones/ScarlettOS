  "python.linting.pylintArgs": [
    "--rcfile",
    "${workspaceFolder}/pylintrc"
  ],
  "python.linting.ignorePatterns": [
    ".vscode/*.py",
    "**/site-packages/**/*.py"
  ],
  "python.venvPath": "~/.virtualenvs",
  "python.sortImports.path": "${env.WORKON_HOME}/scarlett-os-venv2/bin/isort",
